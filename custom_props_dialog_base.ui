      <widget class="QTableView" name="tblProps">
       <property name="styleSheet">
        <string notr="true">QTableView#tblProps {
    background: #ffffff;
    border: 1px solid #c9d6e2;
    border-radius: 10px;
    gridline-color: #e6eef6;
    font-family: &quot;Segoe UI&quot;, &quot;Arial&quot;, sans-serif;
    font-size: 10pt;
    /* testo scuro anche in selezione */
    selection-background-color: #d7ebff;  /* azzurro chiaro */
    selection-color: #0f2a3d;
    alternate-background-color: #fafcff;
    outline: 0; /* niente rettangolo focus per-cella */
}

/* Header */
QHeaderView::section {
    background: #f4f8fb;
    color: #1f2d3d;
    border: none;
    border-right: 1px solid #e6eef6;
    padding: 8px 10px;
    font-weight: 600;
}
QHeaderView::section:last { border-right: none; }
QHeaderView::section:pressed { background: #eaf3fa; }

/* Celle */
QTableView#tblProps::item { padding: 6px 8px; }

/* Hover su riga NON selezionata */
QTableView#tblProps::item:hover {
    background: #f5faff;
    color: #1f2d3d;
}

/* Selezione riga */
QTableView#tblProps::item:selected {
    background: #d7ebff;
    color: #0f2a3d;
}

/* Hover sulla riga selezionata: mantieni leggibile */
QTableView#tblProps::item:selected:hover {
    background: #c9e3ff;
    color: #0f2a3d;
}

/* Scrollbar leggere */
QTableView#tblProps QScrollBar:vertical,
QTableView#tblProps QScrollBar:horizontal { background: transparent; border: none; }
QTableView#tblProps QScrollBar::handle { background: #c9d6e2; border-radius: 6px; }
QTableView#tblProps QScrollBar::handle:hover { background: #a9bdcf; }
QTableView#tblProps QScrollBar:vertical { width: 10px; }
QTableView#tblProps QScrollBar:horizontal { height: 10px; }
</string>
